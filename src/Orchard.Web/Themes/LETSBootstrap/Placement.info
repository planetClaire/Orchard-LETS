<Placement>
  <!-- hide non essential fields on register form -->
  <Match Path="~/Users/Account/RegisterMember/">
    <Place Fields_Common_Text_Edit-Mobile="-"/>
    <Place Fields_Common_Text_Edit-About="-"/>
    <Place Fields_Link_Edit="-"/>
    <Place Parts_MemberLinks_Edit="-" />
  </Match>
  <Match ContentType="CarouselImage">
    <Place Fields_Numeric-Priority="-"/>
  </Match>
  <Match ContentType="Page">
    <Place Parts_Common_Metadata="-"></Place>
  </Match>
  <Match ContentType="BannerWidget">
    <Place Fields_MediaLibraryPicker="-"></Place>
  </Match>
  <Match ContentType="Notice">
    <Match DisplayType="Summary">
      <Place Fields_Dropzone_Summary="Header:0"></Place>
      <Place Parts_Title_Summary="Content:0"></Place>
      <Place Fields_TaxonomyField="-"></Place>
      <Place Fields_Common_Text_Summary-Description="Content:1"/>
      <Place Parts_Common_Metadata_Summary="Footer:1"></Place>
    </Match>
    <Match DisplayType="Detail">
      <Place Fields_Dropzone="Meta:0"></Place>
      <Place Fields_TaxonomyField="Content:after"></Place>
      <Place Parts_Common_Metadata="Meta:1"></Place>
      <Place Parts_Notice="Meta:2"></Place>
    </Match>
  </Match>
  <Match ContentType="CategoryTerm">
    <Place Parts_Common_Metadata="-"></Place>
  </Match>
</Placement>
